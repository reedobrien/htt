{
	"manifest_version": 2,

	"name" : "Form Exposure",
	"version": "0.1",
 	"background" : {
    	"scripts": ["background.js"]
	},
 	"content_scripts": [
     {
       	"matches": ["*://*/*"],
     	  "js": ["jquery.min.js", "content.js"]
     	}
  	],
	"permissions": [
	    "privacy"
		// "https://ghostery?/"
	],
	"browser_action" : {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	}
}
